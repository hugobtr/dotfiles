general {
	colors = true
	interval = 1
}

order += wireless_wlan0
order += ethernet_enp0s31f6
order += volume_master
order += battery_all
order += time

time {
	format = "%Y-%m-%d %H:%M:%S"
}

battery all {
        format = "%status %percentage %remaining"
}

volume master {
	format = "%devicename: %volume"
	format_muted = "%devicename: muted %volume"
}

ethernet enp0s31f6 {

}

wireless wlan0 {

}
